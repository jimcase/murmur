{
  "name": "murmur",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf build",
    "dev": "npm i && webpack serve --config webpack.dev.js --host 0.0.0.0 --port 3002",
    "build": "webpack --config webpack.prod.js ",
    "build:extension": "webpack --config ./src/popup/webpack.popup.js ",
    "build:cap": "rimraf build && webpack --config webpack.prod.js && npx cap sync",
    "open:android": "npx cap run android",
    "open:ios": "npx cap run ios",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src",
    "format": "npm run lint --fix & yarn prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "build:css": "npx tailwindcss -i ./src/theme/tailwind.css -o ./public/tailwind.css"
  },
